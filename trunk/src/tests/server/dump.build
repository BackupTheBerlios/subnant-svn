<project name="test-dump" default="test-dump">

    <target name="test-dump">
        <nant buildfile="${subnant-root}/src/targets/server/create.build" target="create">
            <properties>
                <property name="no-hooks"    value="true"/>
                <property name="no-svnserve" value="true"/>
            </properties>
        </nant>
        <nant buildfile="${subnant-root}/src/targets/server/dump.build" target="dump">
            <properties>
                <property name="compress" value="false"/>   
            </properties>
        </nant>
        <nant buildfile="${subnant-root}/src/targets/server/dump.build" target="dump">
            <properties>
                <property name="compress" value="9"/>   
            </properties> 
        </nant>
        <nant buildfile="${subnant-root}/src/targets/server/dump.build" target="dump">
            <properties>
                <property name="incremental" value="true"/>   
                <property name="deltas"      value="true"/>   
                <property name="rev"         value="0"/>   
            </properties>
        </nant>
    </target>

</project>
