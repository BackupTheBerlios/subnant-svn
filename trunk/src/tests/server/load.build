<project name="test-load" default="test-load">

    <target name="test-load">
        <nant buildfile="${subnant-root}/src/targets/server/create.build" target="create">
            <properties>
                <property name="no-hooks"    value="true"/>
                <property name="no-svnserve" value="true"/>
            </properties>
        </nant>
        <nant buildfile="${subnant-root}/src/targets/server/dump.build" target="dump">
            <properties>
                <property name="compress" value="false"/>   
                <property name="no-total" value="true"/>   
            </properties>
        </nant>
        <nant buildfile="${subnant-root}/src/targets/server/load.build" target="load">
            <properties>
                <property name="dumps"    value="${repos}"/>   
                <property name="no-total" value="true"/>   
            </properties>
        </nant>
    </target>

</project>
