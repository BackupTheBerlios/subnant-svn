<project name="test-bugtraq" default="test-bugtraq">

    <target name="test-bugtraq">
        <nant buildfile="../../targets/test.build" target="create-workingcopy"/>
        <nant buildfile="../../targets/client/bugtraq.build" target="bugtraq"/>
        <nant buildfile="../../targets/test.build" target="commit-workingcopy">
            <properties>
                <property name="commit-should-pass" value="true"/>
            </properties>
        </nant>        
        <nant buildfile="../../targets/test.build" target="proplist">
            <properties>
                <property name="propfind" value="bugtraq"/>
            </properties>
        </nant>        
    </target>

</project>
